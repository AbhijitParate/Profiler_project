<?xml version="1.0" encoding="utf-8"?>
<resources>


    <style name="AppTheme" parent="android:Theme.Material.Light.DarkActionBar">
        <item name="android:colorPrimary">#FFFF8800</item>
        <item name="android:colorPrimaryDark">#e67700</item>
        <item name="android:colorAccent">#FFFF8800</item>
        <item name="android:colorControlHighlight">#FFFF8800</item>
    </style>

    <style name="FloatingActionButton" parent="android:Widget.Material.Button">
        <item name="android:background">@drawable/fab_background</item>
        <item name="android:stateListAnimator">@animator/fab_state_list_animator</item>
        <item name="android:layout_width">56dp</item>
        <item name="android:layout_height">56dp</item>
    </style>

    <style name="FloatingActionButton.Options">
        <item name="android:layout_width">40dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_margin">8dp</item>
    </style>




</resources>
